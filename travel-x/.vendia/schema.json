{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Immigration Data",
  "description": "Uni schema to support Immigration dashboard",
  "type": "object",
  "properties": {
    "People": {
      "description": "DMV Agency",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "DMV": {
            "description": "DMV Agency",
            "type": "object",
            "properties": {
              "name": {
                "description": "Immigrant's Full Name",
                "type": "string"
              },
              "dob": {
                "description": "Immigrant's DOB",
                "type": "string"
              },
              "dlNumber": {
                "description": "Immigrant's dlNumber",
                "type": "string"
              },
              "imageId": {
                "description": "ID for the Immigrant's image",
                "type": "string"
              }
            }
          },
          "SS": {
            "description": "SS Agency",
            "type": "object",
            "properties": {
              "name": {
                "description": "Immigrant's Full Name",
                "type": "string"
              },
              "dob": {
                "description": "Immigrant's DOB",
                "type": "string"
              },
              "ssNum": {
                "description": "Immigrant's ss#",
                "type": "string"
              }
            }
          },
          "DOS": {
            "description": "DOS Agency",
            "type": "object",
            "properties": {
              "name": {
                "description": "Immigrant's Full Name",
                "type": "string"
              },
              "dob": {
                "description": "Immigrant's DOB",
                "type": "string"
              },
              "passportNumber": {
                "description": "Immigrant's Passport Number",
                "type": "string"
              },
              "passportExp": {
                "description": "Immigrant's Passport Expiration",
                "type": "string"
              },
              "imageId": {
                "description": "ID for the Immigrant's image",
                "type": "string"
              }
            }
          },
          "DOT": {
            "description": "DOT Agency",
            "type": "object",
            "properties": {
              "departTime": {
                "description": "Immigrant's Departure Time",
                "type": "string"
              },
              "arrivalTime": {
                "description": "Immigrant's Arrival Time",
                "type": "string"
              },
              "flightNum": {
                "description": "Immigrant's Flight Number",
                "type": "string"
              },
              "manifest": {
                "description": "Immigrant's Flight Manifest",
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}