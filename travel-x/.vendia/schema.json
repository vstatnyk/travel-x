{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Immigration Data",
  "description": "Uni schema to support Immigration dashboard",
  "type": "object",
  "properties": {
    "DMV": {
      "description": "DMV Agency",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "description": "Immigrant's Full Name",
            "type": "string"
          },
          "dlNumber": {
            "description": "Immigrant's DL Number",
            "type": "string"
          },
          "dob": {
            "description": "Immigrant's DOB",
            "type": "string"
          }
        }
      }
    },
    "SS": {
      "description": "SS Agency",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "description": "Immigrant's Full Name",
            "type": "string"
          },
          "dob": {
            "description": "Immigrant's DOB",
            "type": "string"
          },
          "ssn": {
            "description": "Immigrant's ssn",
            "type": "string"
          }
        }
      }
    },
    "DOS": {
      "description": "DOS Agency",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "description": "Immigrant's Full Name",
            "type": "string"
          },
          "dob": {
            "description": "Immigrant's DOB",
            "type": "string"
          },
          "passportNumber": {
            "description": "Immigrant's Passport Number",
            "type": "string"
          },
          "passportExp": {
            "description": "Immigrant's Passport Expiration",
            "type": "string"
          }
        }
      }
    }
  }
}